{
  "name": "vite-react-ssr-typescript",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx src/server",
    "build": "pnpm run clean && pnpm build:client && pnpm build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server1": "vite build --outDir dist/server --ssr ./entry-server.tsx",
    "build:server": "tsc -p tsconfig.server.json && vite build --ssr ./src/entry-server.tsx --outDir dist/server",
    "preview": "pnpm build && cross-env NODE_ENV=production pnpm tsx server",
    "serve": "cross-env NODE_ENV=production node dist/app/server.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "artplayer": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "embla-carousel-react": "^8.0.0",
    "i18next": "^23.10.1",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.5.0",
    "isomorphic-fetch": "^3.0.0",
    "lucide-react": "^0.363.0",
    "nes.css": "^2.3.0",
    "nes.icons": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.1.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "4.17.21",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/serve-static": "^1.15.5",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "4.19.2",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.4.3",
    "tsx": "4.7.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-plugin-css-injected-by-js": "3.5.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  }
}
